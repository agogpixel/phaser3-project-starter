{
  "name": "@agogpixel/phaser3-project-starter",
  "version": "0.4.0",
  "description": "Phaser 3 project starter.",
  "scripts": {
    "build": "cross-env NODE_ENV=production webpack --progress --color",
    "start": "webpack-dev-server --progress --color",
    "test": "jest",
    "lint": "eslint jest.config.js webpack.config.js src && prettier --check .",
    "lint:fix": "eslint --fix jest.config.js webpack.config.js src && prettier --write .",
    "prepare": "husky install"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/agogpixel/phaser3-project-starter.git"
  },
  "author": "kidthales <kidthales@agogpixel.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/agogpixel/phaser3-project-starter/issues"
  },
  "homepage": "https://github.com/agogpixel/phaser3-project-starter#readme",
  "dependencies": {
    "phaser": "^3.55.2"
  },
  "devDependencies": {
    "@types/jest": "^27.0.2",
    "@types/node": "^14.17.27",
    "@typescript-eslint/eslint-plugin": "^5.1.0",
    "@typescript-eslint/parser": "^5.1.0",
    "clean-webpack-plugin": "^4.0.0",
    "copy-webpack-plugin": "^6.4.1",
    "cross-env": "^7.0.3",
    "ejs-loader": "^0.5.0",
    "eslint": "^8.0.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "html-webpack-plugin": "^4.5.2",
    "husky": "^7.0.2",
    "jest": "^27.3.1",
    "jest-canvas-mock": "^2.3.1",
    "lint-staged": "^11.2.3",
    "prettier": "^2.4.1",
    "shx": "^0.3.3",
    "source-map-loader": "^1.1.3",
    "terser-webpack-plugin": "^4.2.3",
    "ts-jest": "^27.0.7",
    "ts-loader": "^8.2.0",
    "tslib": "^2.3.1",
    "typescript": "^4.4.4",
    "webpack": "^4.46.0",
    "webpack-cli": "^3.3.12",
    "webpack-dev-server": "^3.11.2"
  },
  "engines": {
    "node": ">=14.0.0",
    "npm": ">=6.0.0"
  },
  "lint-staged": {
    "*.(html|js|ts)": "eslint --cache --fix",
    "*.(css|less|scss|sass|styl|md|json)": "prettier --write"
  }
